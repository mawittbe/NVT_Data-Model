@prefix nvto: <http://lod.iti-germany.de/schema/nvto/> .
@prefix ex: <http://lod.iti-germany.de/nvto/resource/examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix schema: <http://schema.org/> .
@prefix un: <http://undefined.un/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

###############################################
###   Inverse Relationen                    ###
###############################################

un:isRecordedThrough rdf:type owl:ObjectProperty ;
                      owl:inverseOf un:records .

un:isUsedDuring rdf:type owl:ObjectProperty ;
                owl:inverseOf un:uses .

nvto:containsReference owl:inverseOf un:isReferencedBy .
nvto:hasDigitalRepresentation owl:inverseOf un:isDigitalRepresentationOf .
nvto:hasInformationCarrier owl:inverseOf un:isInformationCarrierOf .
dcterms:isPartOf owl:inverseOf un:hasPart .

