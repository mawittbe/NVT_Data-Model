###  http://lod.iti-germany.de/schema/nvto/PROPOSED_hasInfluenceOn
nvto:PROPOSED_hasInfluenceOn rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf nvto:containsReferenceTo ,
                                                edm:hasMet ;
                             owl:inverseOf nvto:PROPOSED_isInfluencedBy ;
                             rdfs:label "has influence on"@en ,
                                        "hat beeinflusst"@de ;
                             skos:definition "Connects a resource with another resource that it influenced."@en ,
                                             "Verbindet eine Ressource mit einer anderen Ressource, die sie beeinflusst hat."@de .


