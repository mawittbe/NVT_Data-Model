@prefix : <#> .
@prefix edm: <http://www.europeana.eu/schemas/edm/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix nvto: <http://lod.iti-germany.de/schema/nvto/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix un: <http://undefined.un/> .
@base <http://www.w3.org/2002/07/owl#> .


[ rdf:type owl:Ontology
 ] .

#################################################################
#    Object Properties
#################################################################

dcterms:creator owl:propertyChainAxiom (
                        [ owl:inverseOf nvto:hasInformationCarrier ]
                        nvto:hasRecordist
                       ) .

#################################################################
#    Classes
#################################################################

###  http://lod.iti-germany.de/schema/nvto/Agent
nvto:Agent rdf:type owl:Class ;
           rdfs:subClassOf edm:Agent .


###  http://lod.iti-germany.de/schema/nvto/PhysicalObject
nvto:PhysicalObject rdf:type owl:Class ;
                    rdfs:subClassOf edm:ProvidedCHO .


###  http://lod.iti-germany.de/schema/nvto/Place
nvto:Place rdf:type owl:Class ;
           rdfs:subClassOf edm:Place .


###  http://www.europeana.eu/schemas/edm/wasPresentAt
dcterms:isPartOf rdf:type owl:TransitiveProperty .

###  http://www.europeana.eu/schemas/edm/wasPresentAt
edm:wasPresentAt owl:propertyChainAxiom (
                          [ owl:inverseOf nvto:hasInformationCarrier ]
                          [ owl:inverseOf dcterms:isPartOf ]
                          nvto:containsReferenceTo
                        ) ,
                        (
                          [ owl:inverseOf nvto:hasInformationCarrier ]
                          nvto:containsReferenceTo
                        ) ,
                        (
                          nvto:isInformationCarrierOf
                          un:records
                        ) ,
                        (
                          [ owl:inverseOf nvto:hasInformationCarrier ]
                          un:records
                        ) .

###  http://www.europeana.eu/schemas/edm/hasMet
edm:hasMet rdf:type owl:SymmetricProperty ;
            owl:propertyChainAxiom (
                          edm:wasPresentAt
                          edm:happenedAt
                        ) ,
                        (
                          edm:wasPresentAt
                          nvto:hasPerformer
                        ) .

