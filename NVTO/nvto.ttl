<http://ld.iti-germany.de/schema/nvto/> rdf:type owl:Ontology ;
                                        dcterms:contributor "Jonas Pitz" , "Maxim Wittenbecher" ;
                                        dc:title "Non-Verbal Theatre Ontology"@en ;
                                        dc:title "Ontologie für nonverbales Theater"@de ;
                                        dcterms:publisher "Media Library for Dance and Theatre / ITI Germany"@en ;
                                        dcterms:publisher "Mediathek für Tanz und Theater / ITI Deutschland"@de ;
                                        rdfs:label "Non-Verbal Theatre Ontology"@en ;
                                        rdfs:label "Ontologie für nonverbales Theater"@de ;
                                        dc:rights <https://creativecommons.org/licenses/by/4.0/deed.en> ;
                                        dcterms:abstract """Linked-Data ontology for modelling cultural heritage information in the domain of performing arts focused on non-verbal form of theatre. The ontology development is part of the \"NONVERBALES THEATER
                                                          IM EHEMALIGEN OSTTEIL BERLINS 1970-1991\" digitisation project at the Media Library for Dance and Theatre of the German Centre of the International Theatre Institute. The project is funded by the Förderprogramm Digitalisierung von Objekten des kulturellen Erbes Berlin of the Forschungs- und Kompetenzzentrum Digitalisierung Berlin (digiS)."""@en ;
                                        dcterms:abstract """Linked-Data Ontologie für die Modellierung von Informationen über Objekte und Ereignisse des kulturellen Erbes aus dem Bereich der darstellenden Künste mit Fokus auf nonverbale Formen von Theater. Die Entwicklung der Ontologie is Teil des Digitalisierungsprojekts \"NONVERBALES THEATER
                                                          IM EHEMALIGEN OSTTEIL BERLINS 1970-1991\" an der Mediathek für Tanz und Theater am deutschen Zentrum des Internationales Theaterinstituts. Das Projekt wird gefördert durch das Förderprogramm Digitalisierung von Objekten des kulturellen Erbes Berlin des Forschungs- und Kompetenzzentrums Digitalisierung Berlin (digiS)."""@de .

## CLASSES


## NVTO CLASSES

nvto:Agent rdf:type owl:Class ;
           rdfs:label "Agens"@de ;
           rdfs:label "Agent"@en ;
           skos:definition """Diese Klasse umfasst alle Agenzien wie etwa Personen und Institutionen."""@de ;
           skos:definition """This class comprises all agents such as persons or institutions."""@en ;
           skos:example "Burkhart Seidemann, Pantomimen-Theater vom Prenzlauer Berg"@de ;
           skos:example "Burkhart Seidemann, Pantomimen-Theater vom Prenzlauer Berg"@en .

nvto:City rdf:type owl:Class ;
          rdfs:subClassOf nvto:Place ;
          rdfs:label "Stadt"@de ;
          rdfs:label "City"@en ;
          skos:definition """Diese Klasse umfasst Städte abhängig von Zeit und/oder Geopolitik."""@de ;
          skos:definition """This class comprises cities dependent on time and/or geopolitics."""@en ;
          skos:example "Berlin, Westberlin, Ostberlin"@de ;
          skos:example "Berlin, West Berlin, East Berlin"@en .

nvto:Collection rdf:type owl:Class ;
                rdfs:label "Sammlung"@de ;
                rdfs:label "Collection"@en ;
                skos:definition """Diese Klasse umfasst (benannte) Sammlungen von Ressourcen in willkürlicher Reihenfolge."""@de ;
                skos:definition """This class comprises (named) resource collections in arbitrary order."""@en ;
                skos:example "COL"@de ;
                skos:example "COL"@en .

nvto:Country rdf:type owl:Class ;
             rdfs:subClassOf nvto:Place ;
             rdfs:label "Land"@de ;
             rdfs:label "Country"@en ;
             skos:definition """Diese Klasse umfasst Länder abhängig von Zeit und/oder Geopolitik."""@de ;
             skos:definition """This class comprises countries dependent on time and/or geopolitics."""@en ;
             skos:example "Französische Republik, Westdeutschland, DDR"@de  ;
             skos:example "FRG, West Germany, GDR"@en  .

nvto:DigitalObject rdf:type owl:Class ;
                   rdfs:label "Digitales Objekt"@de ;
                   rdfs:label "Digital Object"@en ;
                   skos:definition """Die digitale Repräsentation des Inhalts eines Mediums. Digitaler Informationsträger."""@de ;
                   skos:definition """The digital representation of the contents of a medium. Digital information carrier."""@en ;
                   skos:example "Eine spezifische digitale Datei mit Namen IMG-001.tif, VID-001.avi"@de ;
                   skos:example "A specific digital file with name IMG-001.tif, VID-001.avi"@en .

nvto:Event rdf:type owl:Class ;
           rdfs:label "Ereignis"@de ;
           rdfs:label "Event"@en ;
           skos:definition """Diese Klasse umfasst alle Ereignisse."""@de ;
           skos:definition """This class comprises all events."""@en ;
           skos:scopeNote "Speziell für Ereignisse im Bereich der darstellenden Künste genutzt."@de ;
           skos:scopeNote "Used specifically for events in the realm of performing arts."@en ;
           skos:example "Eine Aufführung von 'Hamlet', ein Workshop zu einer speziellen Schauspieltechnik, eine Ausstellung über eine/n Darsteller/in"@de ;
           skos:example "A performance of 'Hamlet', a workshop regarding a specific acting technique, an exhibition about an actor/actress"@en .

nvto:Festival rdf:type owl:Class ;
              rdfs:label "Festival"@de ;
              rdfs:label "Festival"@en ;
              skos:definition """Diese Klasse umfasst alle Festivals."""@de ;
              skos:definition """This class comprises all festivals."""@en ;
              skos:scopeNote "Speziell für Festivals im Bereich der darstellenden Künste genutzt."@de ;
              skos:scopeNote "Used specifically for festivals in the realm of performing arts."@en ;
              skos:example "Das zweijährliche internationale Theaterfestival 'Theater der Welt'"@de ;
              skos:example "The biannual international theatre festival 'Theater der Welt'"@en .

nvto:FestivalEdition rdf:type owl:Class ;
                     rdfs:subClassOf nvto:Event ;
                     rdfs:label "Festivalausgabe"@de ;
                     rdfs:label "Festival Edition"@en ;
                     skos:definition """Diese Klasse umfasst alle einzelnen Ausgaben eines Festivals."""@de ;
                     skos:definition """This class comprises all singular editions of a Festival."""@en ;
                     skos:scopeNote "Speziell für Festivalausgaben im Bereich der darstellenden Künste genutzt."@de ;
                     skos:scopeNote "Used specifically for festival editions in the realm of performing arts."@en ;
                     skos:example "Die 2017er Ausgabe des zweijährlichen internationale Theaterfestival 'Theater der Welt' in Hamburg, Deutschland"@de ;
                     skos:example "The 2017 edition of the biannual theatre festival 'Theater der Welt', held in Hamburg, Germany"@en .

nvto:InformationObject rdf:type owl:Class ;
                       rdfs:label "Informationsobjekt"@de ;
                       rdfs:label "Information Object"@en ;
                       skos:definition """Konzeptuelle Repräsentation des Inhalts eines Mediums, erfasst von einem physischen oder digitalen (Archiv-)Objekt. Dasselbe Informationsobjekt kann von verschiedenen physischen oder digitalen Objekten (zum Beispiel einer Ausgabe in einem Magazin) erfasst werden. Ein physisches oder digitales Objekt kann außerdem mehere Informationsobjekte tragen (z.B. mehrere Videos auf einer einzelnen Videokassette)"""@de ;
                       skos:definition """Conceptual representation of the contents of a medium captured by a physical or digital (archival) object. The same information object can be captured in multiple pyhsical or digital objects (e.g. a magazine edition). Also one physical or digital object can carry multiple information objects (e.g. multiple movies in a single video tape)."""@en ;
                       skos:example "Aufnahme von Aufführung X, Inhalt von Programmheft Y"@de ;
                       skos:example "Recording of Performance X, Contents of programme booklet Y"@en .

nvto:Organization rdf:type owl:Class ;
                  rdfs:subClassOf nvto:Agent ;
                  rdfs:label "Organisation"@de ;
                  rdfs:label "Organization"@en ;
                  skos:definition """Diese Klasse umfasst alle Organisationen."""@de ;
                  skos:definition """This class comprises all organizations"""@en ;
                  skos:example "Theater der Welt, Sony BMG"@de ;
                  skos:example "Theater der Welt, Sony BMG"@en .

nvto:Performance rdf:type owl:Class ;
           rdfs:subClassOf nvto:Event ;
           rdfs:label "Aufführung"@de ;
           rdfs:label "Performance"@en ;
           skos:definition """Diese Klasse umfasst alle Aufführungen."""@de ;
           skos:definition """This class comprises all performances."""@en ;
           skos:scopeNote "Speziell für Aufführungen im Bereich der darstellenden Künste genutzt."@de ;
           skos:scopeNote "Used specifically for performances in the realm of performing arts."@en ;
           skos:example "Eine Aufführung von 'Hamlet', eine Aufführung von 'Y'"@de ;
           skos:example "A performance of 'Hamlet', a perfomance of 'Y'"@en .

nvto:PerformingArtsGroup rdf:type owl:Class ;
                         rdfs:subClassOf nvto:Agent ;
                         rdfs:label "Gruppe/Ensemble/Company"@de ;
                         rdfs:label "Group/Ensemble/Company"@en ;
                         skos:definition """Diese Klasse umfasst alle Ensembles und ähnliche Gruppen im Bereich der darstellenden Künste."""@de ;
                         skos:definition """This class comprises ensembles and similar groups in the realm of performing arts."""@en ;
                         skos:example "DT Ensemble, Berliner Improvisations Trio, Pantomimen-Theater vom Prenzlauer Berg"@de  ;
                         skos:example "DT Ensemble, Berliner Improvisations Trio, Pantomimen-Theater vom Prenzlauer Berg"@en  .

nvto:PerformingArtsLocation rdf:type owl:Class ;
                            rdfs:subClassOf nvto:Place ;
                            rdfs:label "Aufführungsort"@de ;
                            rdfs:label "Performing Arts Location"@en ;
                            skos:definition """Diese Klasse umfasst Orte, die für darstellende Künste genutzt werden oder wurden."""@de ;
                            skos:definition """This class comprises locations that were or are used for performing arts."""@en ;
                            skos:example "Volksbühne, Pratergarten"@de  ;
                            skos:example "Volksbühne, Pratergarten"@en  .

nvto:PerformingArtsProduction rdf:type owl:Class ;
                              rdfs:label "Produktion"@de ;
                              rdfs:label "Production"@en ;
                              skos:definition """Diese Klasse umfasst Produktionen der darstellenden Kunst, die aufgeführt werden oder wurden."""@de ;
                              skos:definition """This class comprises productions that were or are performed in performing arts events."""@en ;
                              skos:example "Blaubart, Hamlet"@de  ;
                              skos:example "Blaubart, Hamlet"@de  .

nvto:Person rdf:type owl:Class ;
            rdfs:subClassOf nvto:Agent ;
            rdfs:label "Person"@de ;
            rdfs:label "Person"@en ;
            skos:definition """Diese Klasse umfasst alle Personen"""@de ;
            skos:definition """This class comprises all persons"""@en ;
            skos:example "Eberhard Kube, Conny Hege"@de ;
            skos:example "Eberhard Kube, Conny Hege"@en .

nvto:PhysicalObject rdf:type owl:Class ;
                    rdfs:label "Physisches Objekt"@de ;
                    rdfs:label "Physical Object"@en ;
                    skos:definition """Die physische Repräsentation des Inhalts eines Mediums. Physischer Informationsträger."""@de ;
                    skos:definition """The physical representation of the contents of a medium. Physical information carrier. """@en ;
                    skos:example "Videokassette mit Aufnahme von Aufführung X, Programmheft Y"@de ;
                    skos:example "Tape with recording of performance X, programme booklet Y"@en .

nvto:Place rdf:type owl:Class ;
           rdfs:label "Ort"@de ;
           rdfs:label "Place"@en ;
           skos:definition """Diese Klasse umfasst alle Orte."""@de ;
           skos:definition """This class comprises all places."""@en .

nvto:Series rdf:type owl:Class ;
            rdfs:label "Serie"@de ;
            rdfs:label "Series"@en ;
            skos:definition """Diese Klasse umfasst alle Serien."""@en ;
            skos:definition """This class comprises all series."""@en ;
            skos:example "SRS"@de  ;
            skos:example "SRS"@en  .

nvto:TimeSpan rdf:type owl:Class ;
              rdfs:label "Zeitspanne"@de ;
              rdfs:label "Timespan"@en ;
              skos:definition """Diese Klasse umfasst Zeitspannen jeglicher Länge."""@de ;
              skos:definition """This class comprises timespans of any length."""@en ;
              skos:example "1979-1989, 00:00:00:00-00:52:23:17"@de  ;
              skos:example "1979-1989, 00:00:00:00-00:52:23:17"@en  .


## PROPERTIES

nvto:beginsAtTime rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf nvto:hasDate ;
                  rdfs:domain nvto:TimeSpan ;
                  skos:definition "Beschreibt den Startzeitpunkt einer Zeitspanne."@de ;
                  skos:definition "Describes the begin date/point in time of a time span."@en ;
                  rdfs:label "beginnt zu Zeitpunkt"@de ;
                  rdfs:label "begins at time"@en .

nvto:containsAudibleReferenceTo rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf nvto:containsAudioVisualReferenceTo ;
                                skos:definition "Verbindet eine Ressource mit einer anderen Ressource, zu der sie eine hörbare Referenz enthält."@de ;
                                skos:definition "Connects a resource to another resource it contains an audible reference to."@en ;
                                rdfs:label "beinhaltet hörbare Referenz zu"@de ;
                                rdfs:label "contains audible reference to"@en .

nvto:containsAudioVisualReferenceTo rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf nvto:containsReferenceTo ;
                                    skos:definition "Verbindet eine Ressource mit einer anderen Ressource, zu der sie eine audiovisuelle Referenz enthält."@de ;
                                    skos:definition "Connects a resource to another resource it contains an audiovisual reference to."@en ;
                                    rdfs:label "beinhaltet audiovisuelle Referenz zu"@de ;
                                    rdfs:label "contains audiovisual reference to"@en .

nvto:containsReferenceTo rdf:type owl:ObjectProperty ;
                        skos:definition "Verbindet eine Ressource mit einer anderen Ressource, zu der sie eine Referenz enthält."@de ;
                         skos:definition "Connects a resource to another resource it contains any kind of reference to."@en ;
                         skos:scopeNote "Eine Referenz kann in der Aufnahme oder als Beschriftung vorkommen. Wenn die Art der Referenz gegeben ist, sollte eine der Untereigenschaften dieser Eigenschaft verwendet werden."@de ;
                         skos:scopeNote "A reference can occur in the recording or as labeling. In case the type of reference is detailed, one of the subproperties of this property should be used."@en ;
                         rdfs:label "beinhaltet Referenz zu"@de ;
                         rdfs:label "contains reference to"@en .

nvto:containsTextualReferenceTo rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf nvto:containsReferenceTo ;
                                skos:definition "Verbindet eine Ressource mit einer anderen Ressource, zu der sie eine textuelle Referenz enthält."@de ;
                                skos:definition "Connects a resource to another resource it contains a textual reference to."@en ;
                                rdfs:label "beinhaltet textuelle Referenz zu"@de ;
                                rdfs:label "contains textual reference to"@en .

nvto:containsVisualReferenceTo rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf nvto:containsAudioVisualReferenceTo ;
                               skos:definition "Verbindet eine Ressource mit einer anderen Ressource, zu der sie eine visuelle Referenz enthält."@de ;
                               skos:definition "Connects a resource to another resource it contains a visual reference to."@en ;
                               rdfs:label "beinhaltet visuelle Referenz zu"@de ;
                               rdfs:label "contains visual reference to"@en .

nvto:endsAtTime rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf nvto:hasDate ;
                rdfs:domain nvto:TimeSpan ;
                skos:definition "Beschreibt den Endzeitpunkt einer Zeitspanne."@de ;
                skos:definition "Describes the end date/point in time of a time span."@en ;
                rdfs:label "endet zu Zeitpunkt"@de ;
                rdfs:label "ends at time"@en .

nvto:happenedAtPlace rdf:type owl:ObjectProperty ;
