nvto:isPartOf rdf:type owl:ObjectProperty ;
              skos:definition "Verbindet eine Ressource mit einer anderen Ressource, von der sie ein Teil ist."@de ;
              skos:definition "Connects a resource to another resource of which it is a part of."@en ;
              rdfs:label "ist Teil von"@de ;
              rdfs:label "is part of"@en .

nvto:isResidenceOf rdf:type owl:ObjectProperty ;
                   owl:inverseOf nvto:hasResidence ;
                   skos:definition "Verbindet einen Ort mit einem Agens, der an diesem Ort residiert."@de ;
                   skos:definition "Connects a place to an agent that resides at this place."@en ;
                   rdfs:label "ist Residenz von"@de ;
                   rdfs:label "is residence of"@en .

nvto:occurredAtTime rdf:type owl:ObjectProperty ;
                   rdfs:domain nvto:Event ;
                   rdfs:range nvto:TimeSpan ;
                   skos:definition "Verbindet ein Ereignis mit der Zeitspanne, an der es sich ereignete."@de ;
                   skos:definition "Connects an event to the timespan it occurred at."@en ;
                   rdfs:label "ereignete sich zu Zeitpunkt"@de ;
                   rdfs:label "occurred at time"@en .

nvto:originatedAtPlace rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf nvto:hasRelatedPlace ;
                       rdfs:range nvto:Place ;
                       skos:definition "Verbindet eine Ressource mit dem Ort, an dem sie ihren Ursprung hat."@de ;
                       skos:definition "Connects a resource to the location it originated at."@en ;
                       rdfs:label "hat Ursprung in Ort"@de ;
                       rdfs:label "originated at place"@en .

nvto:wasCreatedAtTime rdf:type owl:DatatypeProperty ;
                     skos:definition "Beschreibt einen einzelnen Zeitpunkt, an dem eine Ressource kreiert wurde."@de ;
                     skos:definition "Describes a singular date/point in time at which a resource was created."@en ;
                     rdfs:label "wurde zu Zeitpunkt kreiert"@de ;
                     rdfs:label "was created at time"@en .

nvto:wasIssuedAtTime rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf nvto:hasDate ;
                     skos:definition "Beschreibt einen einzelnen Zeitpunkt, an dem eine Ressource ausgegeben wurde ."@de ;
