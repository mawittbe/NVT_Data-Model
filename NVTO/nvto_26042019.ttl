## CLASSES


## NVTO CLASSES

nvto:Agent rdf:type owl:Class ;
            rdfs:label "Agent"@de ;
            rdfs:label "Agent"@en ;
            skos:definition """This class comprises all Agents such as Persons or Institutions. """ ;
            skos:example "Burkhart Seidemann, Pantomimen-Theater vom Prenzlauer Berg"   .

nvto:Collection rdf:type owl:Class ;
            rdfs:label "Sammlung"@de ;
            rdfs:label "Collection"@en ;
            skos:definition """This class comprises collections. """ ;
            skos:example "COL"   .


nvto:City rdf:type owl:Class ;
      rdfs:subClassOf nvto:Place ;
      rdfs:label "Stadt"@de ;
      rdfs:label "City"@en ;
      skos:definition """This class comprises cities dependent on time and/or geopolitics.""" ;
      skos:example "Berlin, Berlin West, Berlin Ost" .


nvto:Collection rdf:type owl:Class ;
            rdfs:label "Sammlung"@de ;
            rdfs:label "Collection"@en ;
            skos:definition """This class comprises collections. """ ;
            skos:example "COL"   .

nvto:Country rdf:type owl:Class ;
         rdfs:subClassOf nvto:Place ;
         rdfs:label "Land"@de ;
         rdfs:label "Country"@en ;
         skos:definition """This class comprises countries dependent on time and/or geopolitics. """ ;
         skos:example "FRG, West Germany, GDR"  .

nvto:DigitalObject rdf:type owl:Class ;
              rdfs:label "Digitales Objekt"@de ;
              rdfs:label "Digital Object"@en ;
              skos:definition """digital representation of the contents a medium""" ;
              skos:example "IMG.jpg, VID.mp4" .

nvto:Event rdf:type owl:Class ;
          rdfs:label "Ereignis"@de ;
          rdfs:label "Event"@en ;
          skos:definition """this class comprises all events""" ;
          skos:example "a performance of Hamlet" .

nvto:InformationObject rdf:type owl:Class ;
              rdfs:label "Informationsobjekt"@de ;
              rdfs:label "Information Object"@en ;
              skos:definition """conceptual representation of the contents a medium""" ;
              skos:example "Aufnahme von Auff端hrung, Inhalt von Programmheft X" .

nvto:PerformingArtsGroup rdf:type owl:Class ;
                     rdfs:subClassOf nvto:Agent ;
                     rdfs:label "Gruppe/Ensemble"@de ;
                     rdfs:label "Group/Ensemble"@en ;
                     skos:definition """This class comprises ensembles and similar groups in the realm of performing arts.""" ;
                     skos:example "DT, Berliner Improvisations Trio"  .

nvto:PerformingArtsLocation rdf:type owl:Class ;
                        rdfs:subClassOf nvto:Place ;
                        rdfs:label "Auff端hrungsort"@de ;
                        rdfs:label "Performing Arts Location"@en ;
                        skos:definition """This class comprises locations that were or are used for performing arts. """ ;
                        skos:example "Volksb端hne, Pratergarten"  .

nvto:PerformingArtsProduction rdf:type owl:Class ;
                          rdfs:label "Produktion"@de ;
                          rdfs:label "Production"@en ;
                          skos:definition """This class comprises productions that were or are performed. """ ;
                          skos:example "Blaubart, Hamlet"  .

nvto:PhysicalObject rdf:type owl:Class ;
                  rdfs:label "Physisches Objekt"@de ;
                  rdfs:label "Physical Object"@en ;
                  skos:definition """physical representation of the contents a medium""" ;
                  skos:example "Videokassette mit Aufnahme von Auff端hrung, Programmheft X" .

nvto:Place rdf:type owl:Class ;
      rdfs:label "Ort"@de ;
      rdfs:label "Place"@en ;
      skos:definition """This class comprises all Places.""" .

nvto:Series rdf:type owl:Class ;
        rdfs:label "Serie"@de ;
        rdfs:label "Series"@en ;
        skos:definition """This class comprises series. """ ;
        skos:example "SRS"  .

nvto:TimeSpan rdf:type owl:Class ;
        rdfs:label "Zeitspanne"@de ;
        rdfs:label "Timespan"@en ;
        skos:definition """This class comprises timespans of any length. """ ;
        skos:example "1979-1989, 00:52:23"  .


## PROPERTIES

nvto:beginsAtTime rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf dcterms:date ;
                rdfs:domain nvto:TimeSpan ;
                rdfs:comment "describes the begin date of a time span"@en ;
                rdfs:label "begins at time"@en .

nvto:containsAudibleReference rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf nvto:containsAudioVisualReference ;
                      rdfs:comment "describes the connection between two resources in which one resource is audible in the other"@en ;
                      rdfs:label "contains audible reference"@en .

nvto:containsAudioVisualReference rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf nvto:containsReference ;
                      rdfs:comment "describes the connection between two resources in which one resource is either audible or visible or both in the other"@en ;
                      rdfs:label "contains audiovisual reference"@en .

nvto:containsReference rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf dcterms:references ;
                      rdfs:comment "describes the connection between two resources in which one resource contains any kind of reference to the other"@en ;
                      rdfs:label "contains reference"@en .

nvto:containsTextualReference rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf nvto:containsReference ;
                      rdfs:comment "describes the connection between two resources in which one resource is textually referenced in the other"@en ;
                      rdfs:label "contains textual reference"@en .

nvto:containsVisualReference rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf nvto:containsAudioVisualReference ;
                      rdfs:comment "describes the connection between two resources in which one resource is visible in the other"@en ;
                      rdfs:label "contains visual reference"@en .

nvto:endsAtTime rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf dcterms:date ;
                rdfs:domain nvto:TimeSpan ;
                rdfs:comment "describes the end date of a time span"@en ;
                rdfs:label "ends at time"@en .

nvto:happenedAtPlace rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf nvto:hasRelatedPlace ;
                     rdfs:domain nvto:Event ;
                     rdfs:range nvto:Place ;
                     rdfs:comment "connects an event to the place it happened at"@en ;
                     rdfs:label "happened at place"@en .

nvto:hasActor rdf:type owl:ObjectProperty ;
