###  http://lod.iti-germany.de/schema/nvto/Agent
nvto:Agent rdf:type owl:Class ;
           rdfs:label "Agent"@de ,
                      "Agent"@en ;
           skos:definition "This class comprises all Agents such as Persons or Institutions. " ;
           skos:example "Burkhart Seidemann, Pantomimen-Theater vom Prenzlauer Berg" .


###  http://lod.iti-germany.de/schema/nvto/City
nvto:City rdf:type owl:Class ;
          rdfs:subClassOf nvto:Place ;
          rdfs:label "City"@en ,
                     "Stadt"@de ;
          skos:definition "This class comprises cities dependent on time and/or geopolitics." ;
          skos:example "Berlin, Berlin West, Berlin Ost" .


###  http://lod.iti-germany.de/schema/nvto/Collection
nvto:Collection rdf:type owl:Class ;
                rdfs:label "Collection"@en ,
                           "Sammlung"@de ;
                skos:definition """This class comprises collections.
A generic class for (named) resource collections with arbitrary order."""@en ;
                skos:example "COL" .


###  http://lod.iti-germany.de/schema/nvto/Country
nvto:Country rdf:type owl:Class ;
             rdfs:subClassOf nvto:Place ;
             rdfs:label "Country"@en ,
                        "Land"@de ;
             skos:definition "This class comprises countries dependent on time and/or geopolitics. " ;
             skos:example "FRG, West Germany, GDR" .


