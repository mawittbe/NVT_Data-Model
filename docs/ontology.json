  "http://purl.org/dc/elements/1.1/title" : [ {
    "@language" : "en\"",
    "@value" : "Non-Verbal Theatre Ontology"
  }, {
    "@language" : "de\"",
    "@value" : "Ontologie für nonverbales Theater"
  } ],
  "http://purl.org/dc/terms/abstract" : [ {
    "@language" : "de",
    "@value" : "Linked-Data Ontologie für die Modellierung von Informationen über Objekte und Ereignisse des kulturellen Erbes aus dem Bereich der darstellenden Künste mit Fokus auf nonverbale Formen von Theater. Die Entwicklung der Ontologie is Teil des Digitalisierungsprojekts \"NONVERBALES THEATER\n                                                            IM EHEMALIGEN OSTTEIL BERLINS 1970-1991\" an der Mediathek für Tanz und Theater am deutschen Zentrum des Internationales Theaterinstituts. Das Projekt wird gefördert durch das Förderprogramm Digitalisierung von Objekten des kulturellen Erbes Berlin des Forschungs- und Kompetenzzentrums Digitalisierung Berlin (digiS)."
  }, {
    "@language" : "en",
    "@value" : "Linked-Data ontology for modelling cultural heritage information in the domain of performing arts focused on non-verbal form of theatre. The ontology development is part of the \"NONVERBALES THEATER\n                                                            IM EHEMALIGEN OSTTEIL BERLINS 1970-1991\" digitisation project at the Media Library for Dance and Theatre of the German Centre of the International Theatre Institute. The project is funded by the Förderprogramm Digitalisierung von Objekten des kulturellen Erbes Berlin of the Forschungs- und Kompetenzzentrum Digitalisierung Berlin (digiS)."
  } ],
  "http://purl.org/dc/terms/contributor" : [ {
    "@value" : "Jonas Pitz"
  }, {
    "@value" : "Maxim Wittenbecher"
  } ],
  "http://purl.org/dc/terms/publisher" : [ {
    "@language" : "en\"",
    "@value" : "Media Library for Dance and Theatre / ITI Germany"
  }, {
    "@language" : "de\"",
    "@value" : "Mediathek für Tanz und Theater / ITI Deutschland"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Non-Verbal Theatre Ontology"
  }, {
    "@language" : "de",
    "@value" : "Ontologie für nonverbales Theater"
  } ]
